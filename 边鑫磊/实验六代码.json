{
  "constants": [
    {
      "name": "durationTime",
      "value": 1000
    }
  ],
  "charts": [
    {
      "source": "./charts/mushrooms.dsvg"
    }
  ],
  "animations": [
    {
  "selector": ".symbol:not(.IsEdible-0)",
  "grouping": {
    "reference": "start after previous",
    "groupBy": "Surface",
    "sort": {
      "order": ["Fibrous", "Scaly", "Smooth"]
    },
    "grouping": {
      "groupBy": "Odor",
      "reference": "start after previous",
      "sort": {
        "order": ["Almond", "Anise", "Creosote", "Fishy", "Foul", "None", "Pungent", "Spicy"]
      }
    }
  },
  "effects": [
    {
      "type": "wipe left",
      "duration": "durationTime"
    }
  ]
}
  ]
}